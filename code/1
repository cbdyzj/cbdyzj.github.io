// js sleep
const sleep = ms => new Promise(resolve => setTimeout(resolve, ms))

// js 父集含子集
const setContains = (set, subset) => subset.every(e => set.includes(e))

# py 父集含子集
setContains = lambda set, subset: [e for e in subset if e not in set] == []

# py fibonacci数
fibonacci = lambda n: fibonacci(n - 1) + fibonacci(n - 2) if n not in [1, 2] else n
